{
  "env": {
    "es6": true
  },
  "ignorePatterns": ["node_modules"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "import"
  ],
  "root": true,
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
  },
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  }
}
